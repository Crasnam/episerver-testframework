<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFrameworks>net48;net472;net471;net461</TargetFrameworks>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <Authors>loremipsumdonec</Authors>
    <PackageProjectUrl>https://github.com/loremipsumdonec/episerver-testframework</PackageProjectUrl>
    <RepositoryUrl>https://github.com/loremipsumdonec/episerver-testframework</RepositoryUrl>
    <PackageTags>Optimizely CMS testing</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup Condition="'$(TargetFramework)'=='net48' Or '$(TargetFramework)'=='netstandard2.0'">
    <PackageReference Include="EPiServer.CMS" Version="11.20.7" />
    <PackageReference Include="EPiServer.CMS.AspNet" Version="11.20.7" />
    <PackageReference Include="EPiServer.CMS.TinyMce" Version="2.13.4" />
    <PackageReference Include="EPiServer.CMS.UI" Version="11.35.3" />
    <PackageReference Include="EPiServer.CMS.UI.AspNetIdentity" Version="11.35.3" />
    <PackageReference Include="EPiServer.Framework" Version="11.20.7" />
  </ItemGroup>

  <ItemGroup Condition="'$(TargetFramework)'=='net472' Or '$(TargetFramework)'=='net471' Or '$(TargetFramework)'=='net461'">
    <PackageReference Include="EPiServer.CMS" Version="11.3.0" />
    <PackageReference Include="EPiServer.CMS.AspNet" Version="11.3.0" />
    <PackageReference Include="EPiServer.CMS.TinyMce" Version="1.1.0" />
    <PackageReference Include="EPiServer.CMS.UI" Version="11.3.0" />
    <PackageReference Include="EPiServer.CMS.UI.AspNetIdentity" Version="11.3.0" />
    <PackageReference Include="EPiServer.Framework" Version="11.3.0" />
    <PackageReference Include="Microsoft.Owin.Security" Version="3.0.1" />
    <PackageReference Include="Microsoft.Owin.Security.Cookies" Version="3.0.1" />
    <PackageReference Include="Microsoft.Owin.Security.OAuth" Version="3.0.1" />
  </ItemGroup>

  <ItemGroup>
    <Reference Include="System.Web.ApplicationServices" />
  </ItemGroup>

</Project>
