    <PackageReference Include="EPiServer.Framework" Version="[11.3.0, 12.0)" />
    <PackageReference Include="EPiServer.ServiceLocation.StructureMap" Version="2.0" />
    <PackageReference Include="Microsoft.Owin.Security" Version="2.2.1" />
    <PackageReference Include="Microsoft.Owin.Security.Cookies" Version="2.2.1" />
    <PackageReference Include="Microsoft.Owin.Security.OAuth" Version="2.2.1" />
  </ItemGroup>
  
  <ItemGroup>
    <Reference Include="System.Web.ApplicationServices" />
  </ItemGroup>

</Project>
